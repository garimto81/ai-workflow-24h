{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install -r src/workflow/requirements.txt"
  },
  "deploy": {
    "startCommand": "cd src/workflow && python new_agent_system.py 1,2,3 9.0",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  }
}